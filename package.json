{
  "name": "Rugbyvault",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preclean": "npm run unwatch",
    "clean": "rm -rf node_modules",
    "postclean": "yarn; npm run clean-cache",
    "clean-cache": "npm run unwatch; rm -fr $TMPDIR/react-*; rm -rf /tmp/metro-bundler-cache-*; rm -rf $TMPDIR/haste-map-react-native-packager-*",
    "eject-theme": "node node_modules/native-base/ejectTheme.js",
    "hard-reset": "npm run unwatch; npm run unlink; npm run pod-uninstall; npm run clean; npm install; npm run clean-cache; react-native link; npm run pod-install",
    "ios": "react-native run-ios --configuration Debug --simulator 'iPhone 8'",
    "ios:ship": "code-push release-react Rugbyvault ios",
    "postios:ship": "npm run promote",
    "pod-destroy": "cd ./ios; pod deintegrate; cd ..",
    "pod-install": "pod install --project-directory=ios/",
    "pod-uninstall": "rm -rf ios/Pods",
    "pod-update": "pod update --project-directory=ios/",
    "postinstall": "rm -f ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json",
    "promote": "code-push promote Rugbyvault Staging Production",
    "reset": "npm run clean",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "unlink": "react-native unlink native-base; react-native unlink react-native-code-push; react-native unlink react-native-splash-screen; react-native unlink tipsi-stripe; react-native unlink react-native-svg; react-native unlink instabug-reactnative; react-native unlink react-native-linear-gradient; react-native unlink react-native-firebase; react-native unlink react-native-keep-awake",
    "unwatch": "watchman watch-del-all",
    "update": "ncu -u -x native-base,react-native,react,tipsi-stripe,instabug-reactnative,react-native-svg; cd rbv-core; ncu -u",
    "update:dry": "ncu"
  },
  "resolutions": {
    "moment": "2.20.1"
  },
  "dependencies": {
    "color": "3.0.0",
    "history": "4.7.2",
    "instabug-reactnative": "8.0.14",
    "lodash": "4.17.4",
    "moment": "2.20.1",
    "moment-duration-format": "2.2.1",
    "native-base": "2.8.0",
    "numeral": "2.0.6",
    "pluralize": "7.0.0",
    "prop-types": "15.6.0",
    "rbv-core": "file:./rbv-core",
    "react": "16.5.1",
    "react-native": "0.57.1",
    "react-native-animatable": "1.2.4",
    "react-native-code-push": "5.4.2",
    "react-native-collapsible": "0.10.0",
    "react-native-datepicker": "1.6.0",
    "react-native-dotenv": "0.2.0",
    "react-native-firebase": "3.2.2",
    "react-native-flip-card": "3.5.2",
    "react-native-keep-awake": "3.1.0",
    "react-native-linear-gradient": "2.4.0",
    "react-native-modal-datetime-picker": "4.13.0",
    "react-native-offline": "3.7.2",
    "react-native-pathjs-charts": "0.0.33",
    "react-native-picker": "4.3.7",
    "react-native-progress": "3.4.0",
    "react-native-responsive-image": "2.2.0",
    "react-native-simple-gauge": "0.1.7",
    "react-native-slider": "0.11.0",
    "react-native-sound": "0.10.9",
    "react-native-splash-screen": "3.1.1",
    "react-native-swiper": "1.5.13",
    "react-native-textinput-effects": "matthewstyers/react-native-textinput-effects",
    "react-redux": "5.0.6",
    "react-router-native": "4.2.0",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.7.2",
    "redux-form": "7.2.1",
    "redux-persist": "5.5.0",
    "redux-storage": "4.1.2",
    "redux-storage-decorator-debounce": "1.1.3",
    "redux-storage-engine-reactnativeasyncstorage": "1.0.5",
    "redux-storage-merger-simple": "1.0.5",
    "redux-thunk": "2.2.0",
    "rmc-picker": "5.0.4",
    "tipsi-stripe": "3.8.1"
  },
  "devDependencies": {
    "metro-react-native-babel-preset": "0.45.6",
    "react-native-debugger-open": "0.3.15",
    "redux-devtools-extension": "2.13.2",
    "remote-redux-devtools": "0.5.12"
  }
}
